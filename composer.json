{
    "name": "koninklijke-collective/my-redirects",
    "description": "TYPO3 Extension: Redirects management",
    "type": "typo3-cms-extension",

    "keywords": [
        "TYPO3",
        "CMS",
        "Serfhos",
        "Benjamin Serfhos",
        "Extbase",
        "Redirects"
    ],
    "homepage": "https://github.com/koninklijke-collective/my_redirects",
    "license": "LGPL-3.0+",
    "require": {
        "typo3/cms-core": ">=9.5.0 <10.0.0",
        "typo3/cms-frontend": ">9.5.0 <10.0.0",
        "typo3/cms-scheduler": ">=9.5.0 <10.0.0"
    },

    "replace": {
        "my_redirects": "self.version",
        "typo3-ter/my-redirects": "self.version"
    },

    "autoload": {
        "psr-4": {
            "KoninklijkeCollective\\MyRedirects\\": "Classes/"
        }
    },

    "authors": [
        {
            "name": "Benjamin Serfhos",
            "email": "benjamin@serfhos.com",
            "homepage": "http://serfhos.com"
        }
    ]
}
